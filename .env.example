# Snowflake Credentials Template
# Copy this file to .env and fill in your actual credentials
# DO NOT commit .env to version control

# Snowflake Connection Details
SF_ACCOUNT=your-account.snowflakecomputing.com
SF_USER=your-username
SF_PASSWORD=your-password
SF_ROLE=ACCOUNTADMIN

# Snowflake Environment Configuration (Parameterized)
# These values are used as defaults and can be overridden per environment
SF_WAREHOUSE=COMPUTE_WH_XS
SF_DATABASE=LOGISTICS_DW_DEV
SF_SCHEMA=ANALYTICS

# dbt Configuration
DBT_TARGET=dev
DBT_THREADS=4

# Environment-specific overrides (optional)
# Uncomment and modify these if you want different databases per environment
# SF_DATABASE_DEV=LOGISTICS_DW_DEV
# SF_DATABASE_STAGING=LOGISTICS_DW_STAGING  
# SF_DATABASE_PROD=LOGISTICS_DW_PROD

# Warehouse overrides (optional)
# SF_WAREHOUSE_DEV=COMPUTE_WH_XS
# SF_WAREHOUSE_STAGING=COMPUTE_WH_SMALL
# SF_WAREHOUSE_PROD=COMPUTE_WH_MEDIUM

# Schema overrides (optional)
# SF_SCHEMA_DEV=ANALYTICS
# SF_SCHEMA_STAGING=ANALYTICS
# SF_SCHEMA_PROD=ANALYTICS

# Production environment overrides (optional)
# SF_PROD_ACCOUNT=your-prod-account.snowflakecomputing.com
# SF_PROD_USER=your-prod-username
# SF_PROD_PASSWORD=your-prod-password
# SF_PROD_ROLE=DBT_PROD_ROLE
# SF_PROD_WAREHOUSE=COMPUTE_WH_MEDIUM
# SF_PROD_DATABASE=LOGISTICS_DW_PROD
# SF_PROD_SCHEMA=ANALYTICS
